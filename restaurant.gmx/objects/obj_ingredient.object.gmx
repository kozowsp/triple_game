            <string>if (selected == false)
{
    return noone;
}

selected = false;

var move_to_table = false;
var new_position_x = noone;
var new_position_y = noone;

with(obj_table)
{
    if(highlighted_field != noone)
    {
        move_to_table = true;
        
        with(highlighted_field)
        {
            new_position_x = x;
            new_position_y = y;        
            instance_destroy();
        }
        
        ds_grid_set(grid, floor(new_position_x - left_position) / floor(field_width), floor(new_position_y - top_position) / floor(field_height), 1);
        
        highlighted_field = noone;
    } 
}

var item_id = id;

if(move_to_table == true)
{
    x = new_position_x;
    y = new_position_y;
    
    if(is_undefined(id))
    {
        show_debug_message("Object id is undefined.");
    }
    else
    {
        show_debug_message("Object id equals " + string(id) + ".");   
        show_debug_message("Item id equals " + string(item_id) + ".");           
    }
    
    with(obj_queue)
    {
        for(var i = 0; i &lt; slots_number; ++i)
        {
            show_debug_message("Slot " + string(i) + " equals " + string(slots[i]) + ".");
            if(slots[i] == item_id)
            {
                slots[i] = noone;
            }
        }
    }
    
    moveable = false;
    
    scr_populate_queue();
}
else // move to queue
{    
    with(obj_queue)
    {
        for(var i = 0; i &lt; slots_number; ++i)
        {
            if(slots[i] == item_id)
            {
                new_position_x = slot_positions[i, 0];
                new_position_y = slot_positions[i, 1];                
            }
        }
    }

    x = new_position_x;
    y = new_position_y;    
}
